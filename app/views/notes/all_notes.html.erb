<% @notes.each do |note| %>

<div class="card text-white bg-dark mb-3">
  <div class="card-body ">
    <blockquote class="blockquote mb-0">
     <p>"<%= note.message%>"</p>
     <footer class="blockquote-footer" style="font-style: italic; color: DarkGray;"><%= note.name %></footer>
   </blockquote>
  </div>

</div>

<% end %>
